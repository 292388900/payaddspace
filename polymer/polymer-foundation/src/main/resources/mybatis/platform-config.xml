	</class>
	<class name="com.payadd.polymer.model.sys.User" table="sys_user"
		map-id="UserResultMap">
	</class>
	<class map-id="CommonFeeResultMap" name="com.payadd.polymer.model.aut.CommonFee" table="aut_common_fee"></class>
	<class map-id="ProductResultMap" name="com.payadd.polymer.model.com.Product" table="com_product"></class>
	<class map-id="ProductRelationResultMap" name="com.payadd.polymer.model.bdm.ProductRelation" table="bdm_product_relation"></class>
